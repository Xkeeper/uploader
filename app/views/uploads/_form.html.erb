<%= form_for @upload, :html => { :class => 'form-horizontal', :onsubmit => "openProgressBar(); return true;" }, :url => '/fast_uploads' do |f| %>
    <%= f.label :filename, :class => 'control-label' %>
      <%= f.file_field :file, :class => 'file_field' %>
    <%= f.submit "Upload", :class => 'btn btn-primary' %>
<% end %>
